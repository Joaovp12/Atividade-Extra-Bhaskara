    private  void  jButtonGoActionPerformed ( java.awt.event . ActionEvent  evt ) { // GEN-FIRST: event_jButtonGoActionPerformed
        readValues ​​();
        if (returnDelta () > =  0,0  && a ! =  0,0 )
        {
            jTextFieldDelta . setText ( String . valueOf (valueOnString (returnDelta ())));
            jTextFieldR1 . setText ( String . valueOf (valueOnString (returnRline ( ' + ' ))));
            jTextFieldR2 . setText ( String . valueOf (valueOnString (returnRline ( ' - ' ))));
        } else
        {
            JOptionPane . showMessageDialog ( null , " Não é possível calcular! " );
        }
        
      
    } // GEN-LAST: event_jButtonGoActionPerformed

    / *
    * funções do programa
    * /
    public  void  readValues ()
    {
        a =  duplo . parseDouble (jTextFieldA . getText ());
        b =  duplo . parseDouble (jTextFieldB . getText ());
        c =  duplo . parseDouble (jTextFieldC . getText ());
    }
    
    public  String  valueOnString ( valor duplo  )
    {
        Resultado da string ;
        resultado =  String . formato ( " % .5f " , valor);
       
       resultado de retorno ;
    }
    
    public  double  getSqrt ( valor duplo  )
    {
        retornar sqrt (valor);
    }
    
    public  double  returnDelta ()
    {
        delta duplo = (b * b) - ( 4  * a * c);
        return delta;
    }
    
    public  double  returnRline ( símbolo char  )
    {
        double expr =  0,0 ;
        if (símbolo ==  ' + ' )
        {
            expr = (( - 1  * b) + getSqrt (returnDelta ())) / ( 2 * a);
        } else  if (símbolo ==  ' - ' )
        {
            expr = (( - 1  * b) - getSqrt (returnDelta ())) / ( 2 * a);
        }
        return expr;
    }
    
    
    
    
